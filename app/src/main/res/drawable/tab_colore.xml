<?xml version="1.0" encoding="utf-8"?>
<!-- Lista de colores para remarcar los TextViews de login y registro -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false" android:state_selected="false" android:color="@color/secondary_text"/>
    <item android:state_pressed="true" android:color="@color/colorAccent"/>
    <item android:state_pressed="false" android:state_selected="true" android:color="@color/colorAccent"/>
    <item android:color="@color/secondary_text"></item>
</selector>
